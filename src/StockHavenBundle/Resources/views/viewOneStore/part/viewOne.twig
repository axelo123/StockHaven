<br>
{% if success is defined %}
    <div class="alert alert-success alert-dismissible">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">
            <span aria-hidden="true">&times;</span>
        </button>
        <strong>Success !</strong> {{ success }}
    </div>
{% endif %}
{% if error is defined %}
    <div class="alert alert-danger alert-dismissible fade in" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
        <strong>Error !</strong> {{ error }}
    </div>
{% endif %}
<div class="box">
    <div class="box-header with-border">
        <h3 class="box-title">Address</h3>
    </div>
    <div class="box-body">
        <table class="table table-bordered">

            {% for oneAddress in address %}
                {% if oneAddress.storeId == store %}
                    <tbody>
                    <tr>
                        <th>nb box</th>
                        <th>Street</th>
                        <th>Number Postal Code</th>
                        <th>Region</th>
                        <th>Country</th>
                    </tr>
                    <tr>
                        <td>{{ oneAddress.nbBox }}</td>
                        <td>{{ oneAddress.street }}</td>
                        <td>{{ oneAddress.postalCodeId.code }}</td>
                        <td>{{ oneAddress.postalCodeId.region }}</td>
                        <td>{{ oneAddress.postalCodeId.countryId.name }}</td>
                    </tr>
                    </tbody>
                {% endif %}
            {% endfor %}
        </table>
    </div>
</div>
<!-- /.box-header -->