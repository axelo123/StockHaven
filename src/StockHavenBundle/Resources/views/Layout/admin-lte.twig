<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block title_lte %}{% endblock %}</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="shortcut icon" href="{{ asset('bundles/smsite/images/stock_synoptic_erp-ConvertImage.ico') }}">
    {% block css %}
        <link rel="stylesheet" type="text/css"href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
        <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css">
        <link rel="stylesheet" type="text/css" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
        <link rel="stylesheet" type="text/css" href="{{ asset("bundles/smadminlte/css/AdminLTE.min.css") }}" />
        <link rel="stylesheet" type="text/css" href="{{ asset("bundles/smadminlte/css/skins/"~ skin_color_lte ~".min.css") }}" />
    {% endblock %}
    {% block script %}
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
        <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.0.2/Chart.min.js"></script>
        <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jQuery-slimScroll/1.3.7/jquery.slimscroll.js"></script>
        <script src="{{ asset("bundles/smadminlte/js/app.min.js") }}"></script>
    {% endblock %}
</head>
<body class="hold-transition {{ skin_color_lte }} sidebar-mini fixed">
<div class="wrapper">
    <header class="main-header">
        <a href="{% block logo_href%}{% endblock %}" onclick="{% block onclick %}{% endblock %}" class="logo">
            <span class="logo-mini">{% block logo_mini_lte %}{% endblock %}</span>
            <span class="logo-lg">{% block logo_lg_lte %}{% endblock %}</span>
        </a>
        <nav class="navbar navbar-static-top" role="navigation">
            <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                <span class="sr-only">Toggle navigation</span>
            </a>
            {% block menu_axelo %}<div class="navbar-custom-menu" >{% block menu_rigth_lte %}{% endblock %}</div>{% endblock %}

        </nav>
    </header>
    <aside class="main-sidebar">{% block main_sidebar_lte %}{% endblock %}</aside>

    <div class="content-wrapper">
        <section id="content-header-lte" class="content-header">{% block content_header_lte %}{% endblock %}</section>
        <section id="content-content-lte" class="content">{% block content_content_lte %}{% endblock %}</section>
    </div>

    <footer class="main-footer">
        <div class="pull-right hidden-xs">{% block footer_rigth_lte %}{% endblock %}</div>
        <div>{% block footer_left_lte %}{% endblock %}</div>
    </footer>

    <aside class="control-sidebar control-sidebar-dark">
        {% block control_sidebar_lte %}
        {% endblock %}</aside>
    <div class="control-sidebar-bg"></div>
</div>
{% block end_body_lte %}
{% endblock %}
</body>
</html>